{
  "license": "MIT",
  "private": false,
  "name": "@dc0de/ts2md",
  "version": "0.0.1",
  "description": "Simple TypeScript to Markdown transpiler",
  "main": "lib/index.js",
  "bin": {
    "ts2md": "./bin/cli.js"
  },
  "repository": "https://github.com/dcodeteam/ts2md.git",
  "author": "Umidbek Karimov <uma.karimov@gmail.com>",
  "scripts": {
    "ci": "yarn lint && yarn dts && yarn cover --ci && yarn build",
    "all": "yarn lint --fix && yarn dts && yarn cover -u && yarn build",
    "dts": "tsc --noEmit",
    "test": "jest --watch",
    "cover": "jest --coverage",
    "lint": "js-scripts lint",
    "start": "yarn build -w",
    "prebuild": "rimraf lib",
    "build": "tsc --project tsconfig.cjs.json",
    "docs": "node bin/cli.js --root src --entry src/index.ts"
  },
  "dependencies": {
    "commander": "^2.17.1",
    "globby": "^8.0.1",
    "lodash": "^4.17.10",
    "prettier": "^1.14.2"
  },
  "devDependencies": {
    "@dc0de/eslint-config": "^0.3.0",
    "@dc0de/eslint-config-base": "^0.3.0",
    "@dc0de/eslint-config-node": "^0.3.0",
    "@dc0de/eslint-config-react": "^0.3.0",
    "@dc0de/js-scripts": "^0.1.12",
    "@types/globby": "^8.0.0",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.7.1",
    "@types/prettier": "^1.13.2",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-react": "^7.11.0",
    "eslint-plugin-typescript": "^0.12.0",
    "jest": "^23.5.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.3",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^18.0.0"
  }
}
